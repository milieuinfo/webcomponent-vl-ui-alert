{
  "name": "vl-ui-alert",
  "version": "1.3.9",
  "description": "De \"alert\" UI component van Webuniversum Vlaanderen als Web Component",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponent-vl-ui-alert.git"
  },
  "author": "DIDM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milieuinfo/webcomponent-vl-ui-alert/issues"
  },
  "scripts": {
    "prepare": "npm update --dev vl-ui-util && npm run prepare:util",
    "prepare:util": "npm explore vl-ui-util -- npm run util",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "wct --config-file ./wct.local.conf.json",
    "test:e2e": "npm run test:server:start && npm run test:e2e:chrome && npm run test:e2e:firefox",
    "test:e2e:grid": "npm run test:server:start && npm run test:e2e:chrome:grid && npm run test:e2e:firefox:grid",
    "test:grid": "wct --config-file ./wct.docker.conf.json --npm && npm run test:e2e:grid",
    "posttest:e2e": "pkill -f http-server",
    "test:e2e:chrome": "mocha test/**/*.test.js --chrome",
    "test:e2e:chrome:grid": "mocha test/**/*.test.js --chrome --grid",
    "test:e2e:firefox": "mocha test/**/*.test.js --firefox",
    "test:e2e:firefox:grid": "mocha test/**/*.test.js --firefox --grid",
    "test:e2e:opera": "mocha test/**/*.test.js --opera",
    "test:e2e:safari": "mocha test/**/*.test.js --safari",
    "test:server:start": "http-server &",
    "release": "np",
    "release:prepare": "npm run release:prepare:build && npm run release:prepare:css && npm run release:prepare:commit",
    "release:prepare:build": "minify *.src.js > vl-alert.js",
    "release:prepare:css": "replace '\\.\\./style.css' '/node_modules/vl-ui-alert/style.css' vl-alert.js",
    "release:prepare:commit": "git add -f vl-alert.js && git commit --amend --no-edit && git pull",
    "release:testless": "np --yolo",
    "demo": "http-server"
  },
  "dependencies": {
    "vl-ui-core": "^3.0.0",
    "vl-ui-icon": "milieuinfo/webcomponent-vl-ui-icon#ovam-poc"
  },
  "peerDependencies": {
    "vl-ui-core": "^3.0.0",
    "vl-ui-icon": "milieuinfo/webcomponent-vl-ui-icon#ovam-poc"
  },
  "devDependencies": {
    "@govflanders/vl-ui-alert": "3.9.1",
    "@govflanders/vl-ui-util": "3.9.1",
    "vl-ui-button": "^1.3.3",
    "vl-ui-util": "^3.4.2"
  },
  "homepage": "https://github.com/milieuinfo/webcomponent-vl-ui-alert#readme",
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  }
}
